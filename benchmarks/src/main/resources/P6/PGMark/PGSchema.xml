<?xml version="1.0"?>
<pgmark>
    <types>
        <type name="node">
            <count>
                <proportion>
                    1.0
                </proportion>
            </count>
            <attributes>
                <attribute name="id_label" unique="false" required="true">
                    <regex>[A-Z][a-z]{1,40}</regex>
                </attribute>
                <attribute name="start_label" unique="false" required="true">
                    <numeric min="0" max="2025">
                        <exponentialDistribution scale="26000"/>
                    </numeric>
                </attribute>
                <attribute name="end_label" unique="false" required="true">
                    <numeric min="0" max="2025">
                        <exponentialDistribution scale="26000"/>
                    </numeric>
                </attribute>
            </attributes>
        </type>

    </types>
    <predicates>

    </predicates>
</pgmark>